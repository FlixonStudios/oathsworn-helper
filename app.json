{
  "expo": {
    "name": "Game Probability",
    "slug": "oathsworn-helper",
    "version": "0.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/game-probability-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/game-probability-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.oathswornadvisor"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Exo2-Regular.ttf",
            "./assets/fonts/Exo2-Italic.ttf",
            "./assets/fonts/Exo2-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/oathsworn-helper/"
    }
  }
}
