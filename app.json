{
  "expo": {
    "name": "Game Probability",
    "slug": "oathsworn-helper",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/game-probability-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/game-probability-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.oathswornadvisor"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/oathsworn-helper/"
    }
  }
}
